<?xml version="1.0" encoding="UTF-8"?>
<openbox_config xmlns="http://openbox.org/3.4/rc">
  <keyboard>
    <chainQuitKey>W-g</chainQuitKey>
    <keybind key="A-Escape">
      <action name="ToggleDockAutohide"/>
    </keybind>
    <!-- Keybindings for desktop switching -->
    <keybind key="W-A-Left">
      <action name="DesktopLeft">
        <dialog>no</dialog>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="W-A-Right">
      <action name="DesktopRight">
        <dialog>no</dialog>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="W-S-Left">
      <action name="SendToDesktopLeft">
        <dialog>no</dialog>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="W-S-Right">
      <action name="SendToDesktopRight">
        <dialog>no</dialog>
        <wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="W-A-Down">
      <action name="ToggleShowDesktop"/>
    </keybind>
    <!-- Keybindings for windows -->
    <keybind key="W-F4">
      <action name="Close"/>
    </keybind>
    <keybind key="W-Down">
      <action name="Iconify"/>
    </keybind>
    <keybind key="W-Escape">
      <action name="ToggleDecorations"/>
    </keybind>
    <keybind key="W-0x23">
      <action name="UnmaximizeFull"/>
      <action name="ResizeRelative">
        <bottom>-1%</bottom>
      </action>
    </keybind>
    <keybind key="W-0x33">
      <action name="UnmaximizeFull"/>
      <action name="ResizeRelative">
        <bottom>1%</bottom>
      </action>
    </keybind>
    <keybind key="W-0x3C">
      <action name="UnmaximizeFull"/>
      <action name="ResizeRelative">
        <right>-1%</right>
      </action>
    </keybind>
    <keybind key="W-0x3D">
      <action name="UnmaximizeFull"/>
      <action name="ResizeRelative">
        <right>1%</right>
      </action>
    </keybind>
    <keybind key="W-0x30">
      <action name="UnmaximizeFull"/>
      <action name="MoveRelative">
        <x>0</x>
        <y>1%</y>
      </action>
    </keybind>
    <keybind key="W-0x22">
      <action name="UnmaximizeFull"/>
      <action name="MoveRelative">
        <x>0</x>
        <y>-1%</y>
      </action>
    </keybind>
    <keybind key="W-m">
      <action name="UnmaximizeFull"/>
      <action name="MoveRelative">
        <x>-1%</x>
        <y>0</y>
      </action>
    </keybind>
    <keybind key="W-0x3B">
      <action name="UnmaximizeFull"/>
      <action name="MoveRelative">
        <x>1%</x>
        <y>0</y>
      </action>
    </keybind>
    <keybind key="W-1">
      <action name="Execute">
        <command>xwinreg_master_west.sh</command>
      </action>
    </keybind>
    <keybind key="W-2">
      <action name="Execute">
        <command>xwinreg_master_north.sh</command>
      </action>
    </keybind>
    <keybind key="W-3">
      <action name="Execute">
        <command>xwinreg_master_east.sh</command>
      </action>
    </keybind>
    <keybind key="W-4">
      <action name="Execute">
        <command>xwinreg_master_south.sh</command>
      </action>
    </keybind>
    <keybind key="W-5">
      <action name="Execute">
        <command>xwinreg_grid_square_horizontal.sh</command>
      </action>
    </keybind>
    <keybind key="W-6">
      <action name="Execute">
        <command>xwinreg_grid_square_vertical.sh</command>
      </action>
    </keybind>
    <keybind key="W-7">
      <action name="Execute">
        <command>xwinreg_grid_horizontal.sh</command>
      </action>
    </keybind>
    <keybind key="W-8">
      <action name="Execute">
        <command>xwinreg_grid_vertical.sh</command>
      </action>
    </keybind>
    <keybind key="W-9">
      <action name="Execute">
        <command>xwinreg_hide_active_region.sh</command>
      </action>
    </keybind>
    <keybind key="W-0">
      <action name="Execute">
        <command>xwinreg_unhide_active_region.sh</command>
      </action>
    </keybind>
    <keybind key="W-0x14">
      <action name="Execute">
        <command>xwinreg_maximize_all.sh</command>
      </action>
    </keybind>
    <keybind key="W-0x5E">
      <action name="Execute">
        <command>xwinreg_focus_toggle_next.sh</command>
      </action>
    </keybind>
    <keybind key="W-S-0x5E">
      <action name="Execute">
        <command>xwinreg_focus_toggle_preview.sh</command>
      </action>
    </keybind>
    <keybind key="W-0x76">
      <action name="Execute">
        <command>xwinreg_cycle_clock.sh</command>
      </action>
    </keybind>
    <keybind key="W-0x6E">
      <action name="Execute">
        <command>xwinreg_cycle_anticlock.sh</command>
      </action>
    </keybind>
    <keybind key="W-0x70">
      <action name="Execute">
        <command>xwinreg_cycle_reverse.sh</command>
      </action>
    </keybind>
    <keybind key="W-0x77">
      <action name="Execute">
        <command>xwinreg_cycle_reg_clock.sh</command>
      </action>
    </keybind>
    <keybind key="W-a">
        <keybind key="C">
        <action name="Execute">
            <command>xwinreg_close_active_region.sh</command>
        </action>
        </keybind>
        <keybind key="F">
        <action name="Execute">
            <command>xwinreg_focus_active_region.sh</command>
        </action>
        </keybind>
    </keybind>
    <keybind key="W-a">
      <!-- Begin the keychains to manage windows -->
      <!-- aeiah's semi-tiling mode -->
      <keybind key="1">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
          <x>0</x>
          <y>0</y>
          <width>50%</width>
          <height>50%</height>
        </action>
      </keybind>
      <keybind key="2">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
          <x>0</x>
          <y>0</y>
          <width>100%</width>
          <height>50%</height>
        </action>
      </keybind>
      <keybind key="3">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
          <x>50%</x>
          <y>0</y>
          <width>50%</width>
          <height>50%</height>
        </action>
      </keybind>
      <keybind key="4">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
          <x>50%</x>
          <y>0</y>
          <width>50%</width>
          <height>100%</height>
        </action>
      </keybind>
      <keybind key="5">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
          <x>50%</x>
          <y>50%</y>
          <width>50%</width>
          <height>50%</height>
        </action>
      </keybind>
      <keybind key="6">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
          <x>0</x>
          <y>50%</y>
          <width>100%</width>
          <height>50%</height>
        </action>
      </keybind>
      <keybind key="7">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
          <x>0</x>
          <y>50%</y>
          <width>50%</width>
          <height>50%</height>
        </action>
      </keybind>
      <keybind key="8">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
          <x>0</x>
          <y>0</y>
          <width>50%</width>
          <height>100%</height>
        </action>
      </keybind>
      <keybind key="9">
        <action name="UnmaximizeFull"/>
        <action name="MoveResizeTo">
          <x>center</x>
          <y>center</y>
          <width>50%</width>
          <height>50%</height>
        </action>
      </keybind>
      <keybind key="Return">
        <action name="ToggleMaximizeFull"/>
      </keybind>
      <keybind key="F12">
        <action name="ToggleFullscreen"/>
      </keybind>
      <keybind key="m">
        <action name="UnmaximizeFull"/>
        <action name="MoveToEdge">
          <direction>west</direction>
        </action>
      </keybind>
      <keybind key="0x3B">
        <action name="UnmaximizeFull"/>
        <action name="MoveToEdge">
          <direction>east</direction>
        </action>
      </keybind>
      <keybind key="0x30">
        <action name="UnmaximizeFull"/>
        <action name="MoveToEdge">
          <direction>south</direction>
        </action>
      </keybind>
      <keybind key="0x22">
        <action name="UnmaximizeFull"/>
        <action name="MoveToEdge">
          <direction>north</direction>
        </action>
      </keybind>
      <keybind key="0x3D">
        <action name="UnmaximizeFull"/>
        <action name="GrowToEdge">
          <direction>east</direction>
        </action>
      </keybind>
      <keybind key="0x33">
        <action name="UnmaximizeFull"/>
        <action name="GrowToEdge">
          <direction>south</direction>
        </action>
      </keybind>
      <keybind key="0x3C">
        <action name="UnmaximizeFull"/>
        <action name="ShrinkToEdge">
          <direction>west</direction>
        </action>
      </keybind>
      <keybind key="0x23">
        <action name="UnmaximizeFull"/>
        <action name="ShrinkToEdge">
          <direction>north</direction>
        </action>
      </keybind>
    </keybind>
    <!-- Keybindings for window switching -->
    <keybind key="W-Tab">
      <action name="NextWindow">
        <raise>yes</raise>
      </action>
    </keybind>
    <keybind key="W-S-Tab">
      <action name="PreviousWindow">
        <raise>yes</raise>
      </action>
    </keybind>
    <keybind key="W-space">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>List xwin-list</name>
        </startupnotify>
        <command>dmenu_xwin_list.sh</command>
      </action>
    </keybind>
    <!-- Keybindings for running menus -->
    <keybind key="A-Tab">
      <action name="ShowMenu">
        <menu>client-list-combined-menu</menu>
      </action>
    </keybind>
    <keybind key="A-space">
      <action name="ShowMenu">
        <menu>root-menu</menu>
      </action>
    </keybind>
    <!-- Keybindings for running applications -->
    <keybind key="W-Print">
      <action name="Execute">
        <execute>scrot '%Y-%m-%d--%s_$wx$h.png' -e 'mv $f ~/Bilder/ &amp; viewnior ~/Bilder/$f'</execute>
      </action>
    </keybind>
    <keybind key="W-A-Print">
      <action name="Execute">
        <execute>scrot -d 10 '%Y-%m-%d--%s_$wx$h.png' -e 'mv $f ~/Bilder/ &amp; viewnior ~/Bilder/$f'</execute>
      </action>
    </keybind>
    <keybind key="W-b">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>Run Program</name>
        </startupnotify>
        <command>gmrun</command>
      </action>
    </keybind>
    <keybind key="W-t">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>Terminal</name>
        </startupnotify>
        <command>xterm</command>
      </action>
    </keybind>
    <keybind key="W-u">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>Terminal</name>
        </startupnotify>
        <command>urxvt</command>
      </action>
    </keybind>
    <keybind key="W-A-l">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>Lock screen</name>
        </startupnotify>
        <command>bash -c "slock"</command>
      </action>
    </keybind>
    <keybind key="W-s">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>xscreensaver</name>
        </startupnotify>
        <command>xscreensaver-demo</command>
      </action>
    </keybind>
    <keybind key="W-v">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>acti.sh</name>
        </startupnotify>
        <command>acti.sh --gui</command>
      </action>
    </keybind>
    <keybind key="W-r">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>arandr</name>
        </startupnotify>
        <command>arandr</command>
      </action>
    </keybind>
  </keyboard>
  <applications>
    <application class="*">
      <decor>no</decor>
    </application>
    <application name="gmessage">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>yes</skip_pager>
      <skip_taskbar>yes</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>no</maximized>
    </application>
    <application name="claws-mail">
      <decor>yes</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>yes</maximized>
    </application>
    <application name="thunar">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>yes</maximized>
    </application>
    <application name="urxv*">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>yes</maximized>
    </application>
    <application name="*xterm">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>yes</maximized>
    </application>
    <application name="openbox-logout">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>yes</skip_pager>
      <fullscreen>no</fullscreen>
      <maximized>no</maximized>
      <layer>above</layer>
    </application>
    <application name="stjerm">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>no</maximized>
    </application>
    <application name="keepassx">
      <decor>yes</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>no</maximized>
    </application>
    <application name="geany">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>yes</maximized>
    </application>
    <application name="qupzilla">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>yes</maximized>
    </application>
    <application class="VirtualBox">
      <decor>yes</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>yes</maximized>
    </application>
    <application class="Emacs*">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>yes</fullscreen>
      <maximized>yes</maximized>
    </application>
    <application name="evince">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>yes</maximized>
    </application>
    <application name="vlc">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>yes</maximized>
    </application>
    <application name="spacefm">
      <decor>no</decor>
      <shade>no</shade>
      <skip_pager>no</skip_pager>
      <skip_taskbar>no</skip_taskbar>
      <fullscreen>no</fullscreen>
      <maximized>yes</maximized>
    </application>
  </applications>
</openbox_config>
